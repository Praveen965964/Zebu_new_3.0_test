  },
    computed: {
          Cards() {
              if (this.tab4) {
                  const searchTerm = this.tab4.toLowerCase();
                  return this.cards.filter(card => card.description.toLowerCase().includes(searchTerm));
              } else {
                  return this.cards;
              }
          },
         
          arrayLength() {
              return this.cards.length;
          },
          
      },
    
  };
